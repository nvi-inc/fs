	rm -f ../bin/pcalimage
	ln -s ../systests/pcalimage ../bin/pcalimage
#
../bin/pcalpower: pcalpower
	rm -f ../bin/pcalpower
	ln -s ../systests/pcalpower ../bin/pcalpower
#
../bin/pcalspur: pcalspur
	rm -f ../bin/pcalspur
	ln -s ../systests/pcalspur ../bin/pcalspur
#
../bin/samplestat: samplestat
	rm -f ../bin/samplestat
	ln -s ../systests/samplestat ../bin/samplestat
#
../bin/plotlog: plotlog
	rm -f ../bin/plotlog
	ln -s ../systests/plotlog ../bin/plotlog
#
../bin/chk1024: chk1024
	rm -f ../bin/chk1024
	ln -s ../systests/chk1024 ../bin/chk1024
