FTN77,I,Y
$CDS ON
      FUNCTION IHX2A(INUM)
C     CONVERT INUM TO A HEX CHARACTER, RETURNED IN LOWER BYTE 
      DIMENSION LHEX(8) 
      DATA LHEX/2H01,2H23,2H45,2H67,2H89,2HAB,2HCD,2HEF/
C 
      IHX2A = 0 
      IF (INUM.LT.0.OR.INUM.GT.15) RETURN 
      IHX2A = JCHAR(LHEX,INUM+1)
      RETURN
      END 
