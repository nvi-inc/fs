FTN77,I,Y
$CDS ON
C@IH22A 
      FUNCTION IH22A(IBYTE) 
C 
C     ROUTINE TO CONVERT LOWER 8 BITS OF IBYTE TO A PRINTABLE A2 HEX. 
C 
      DIMENSION ITAB(8)
      DATA ITAB /'01','23','45','67','89','AB','CD','EF'/
C 
      IH22A=JCHAR(ITAB,IAND(IBYTE,360B)/20B+1)*400B+
     .      JCHAR(ITAB,IAND(IBYTE,17B)+1) 
      RETURN
      END 
