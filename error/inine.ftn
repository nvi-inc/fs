FTN77,I,Y  
$CDS ON
      SUBROUTINE ININE(A,B,NPAR)
      IMPLICIT NONE
      INTEGER*2 NPAR
      REAL*8 A(1),B(1)
C
      INTEGER*2 NXPNT,I,J
C
      NXPNT=0
      DO I=1,NPAR
        B(I)=0.0D0
        DO J=1,I
          A(NXPNT+J)=0.0D0
        ENDDO
        NXPNT=NXPNT+I
      ENDDO
C
      RETURN
      END
