FTN77,I,Y  
$CDS ON
      SUBROUTINE INCNE(A,B,AUX,NPAR,R,W)
      IMPLICIT NONE
      INTEGER*2 NPAR
      REAL*8 A(1),B(1),AUX(1),R,W
C
      INTEGER*2 NXPNT,I,J
C
      NXPNT=0
      DO I=1,NPAR
        B(I)=B(I)+R*AUX(I)*W
        DO J=1,I
          A(NXPNT+J)=A(NXPNT+J)+AUX(I)*AUX(J)*W
        ENDDO
        NXPNT=NXPNT+I
      ENDDO
C
      RETURN
      END
