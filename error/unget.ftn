FTN77,I,Y  
$CDS ON
      SUBROUTINE UNGET(JBUF,IERR,LEN)
      DIMENSION JBUF(1)
C
      LOGICAL KGOT
      COMMON/GOT/KGOT,LENG,IERRG,JBUFG(100)
C
      IF(KGOT) STOP
C
      KGOT=.TRUE.
      LENG=LEN
      IERRG=IERR
      DO 10 I=1,MIN0(LENG,100)
         JBUFG(I)=JBUF(I)
10    CONTINUE
  
      RETURN
      END
