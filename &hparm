FTN4,X
      SUBROUTINE HPARM,INITIALIZE COMMON VARIABLES
C 
      INCLUDE #FSCOM::FS
C 
      DO 100 I=1,2
        FOROFF(I) = 0.
        REVOFF(I) = 0.
        FASTFW(I) = -1800.
        SLOWFW(I) = -10.
        FASTRV(I) = 1800
        SLOWRV(I) = 10
        PSLOPE(I) = 85. 
        NSLOPE(I) = 75. 
100   CONTINUE
      PTOLER = 1. 
      IHEAD = 1 
      IHDNDX = 1
      TCOEFF = 0. 
      TCOREF = 20.
      RETURN
      END 
