# Set aliases
alias a alias
a una unalias
a cd 'cd \!*;set prompt="`hostname`:`pwd`/:> "'
a cls 'clear'
a h history
a lo logout
a lower 'tr "[A-Z]" "[a-z]"'
a lsf 'ls -CF'
a ll 'ls -l'
a mail mailx
a pu 'pushd \!*;set prompt="`hostname`:`pwd`/:> "'
a po 'popd \!*;set prompt="`hostname`:`pwd`/:> "'
a d dirs
a rm 'rm -i'
a psall 'ps -ef | egrep -v "(^  daemon|^    root|^      lp)"'
