FTN77,I,Y  
$CDS ON
      LOGICAL FUNCTION KPOUT(LUT,IDCB,IBUF,NWDS,IOBUF,LST)
      character*(*) iobuf
      LOGICAL KWRIT
C
      IF(LST.GT.0) CALL EXEC(2,LST,IBUF,NWDS)
C
      IWDS=NWDS
      IF(NWDS.LT.0) IWDS=((-NWDS)+1)/2
      CALL WRITF(IDCB,IERR,IBUF,IWDS)
      KPOUT=KWRIT(LUT,IERR,IOBUF)
      END
