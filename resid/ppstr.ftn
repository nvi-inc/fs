FTN77,I,Y  
$CDS ON
      SUBROUTINE PPSTR(IBUF,IX,IY,ISTR,ICH) 
      DIMENSION IBUF(1),ISTR(1) 
C 
C  Put STRING AT PLOTTER Coordinates
C 
C      WRITE(1,9956) IX,IY,ICH                                           DEBUG
C9956  FORMAT(" PPSTR: IX,IY,ICH ",3I7)                                  DEBUG
C 
      DO 10 I=1,ICH 
      CALL PPPNT(IBUF,IX+I-1,IY,JCHAR(ISTR,I))
10    CONTINUE
      RETURN
      END 
