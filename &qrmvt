FTN4,X
C@QRMVT 
      PROGRAM QRMVT,5 
C     This segment of QUIKR deals with moving the tape. 
      INCLUDE #FSCOM::FS
      CALL LINQB(NSUB)
      GOTO (1,2,3,3) NSUB 
1     CALL LINQC
      CALL ST(IP) 
      GOTO 900
2     CALL LINQC
      CALL ET(IP) 
      GOTO 900
3     CALL LINQC
      CALL RWFF(IP,NSUB)  
      GOTO 900
900   CALL LINQD
      END 
