FTN4,X
      SUBROUTINE WET(OLAT,ELEVO,ELEV,TEMP,PRES,RH,WETD),C#870115:04:46# 
C 
C     COBBLED TOGETHER TO COMPUTE THE WET PORTION OF THE ATMOSPHERIC
C     DELAY FROM THE MARINI MODEL 
C     BRS 820524
C     MODIFIED TO CONVERT SINGLE PRECISION INPUT TO DOUBLE
C     NRV 820525
C 
C     INPUT ARGUMENTS 
C 
C     OLAT - SITE LATITUDE (RADIANS) (SHOULD BE GEODETIC) 
C     ELEVO - SITE ELEVATION ABOVE GEOID (METERS) 
C     ELEV - THE ELEVATION ANGLE OF THE SOURCE (RADIANS)
C     TEMP - TEMPERATURE (DEGREES C)
C     PRES - ATMOSPHERIC PRESSURE (MILLIBARS) 
C     RH - RELATIVE HUMIDITY (0-1, 50% = 0.5) 
C 
C     OUTPUT ARGUMENTS
C 
C     WETD - THE WET PORTION OF THE MARINI DELAY (CM)       
C 
C     LOCAL VARIABLES 
C 
      DOUBLE PRECISION DOLAT,DELEVO,DELEV,DTEMP,DPRES,DRH 
      DOUBLE PRECISION DATMM,DDRY 
C 
      DOLAT = OLAT
      DELEVO = ELEVO
      DELEV = ELEV
      DTEMP = TEMP
      DPRES = PRES
      DRH = RH
D     WRITE(1,9901) DELEV,DPRES,DTEMP,DRH,DOLAT,DELEVO
D9901 FORMAT("CALLING METO WITH ELEV,PRES,TEMP,RH,OLAT,ELEV="/  
D    .6D20.6) 
      CALL METO3(DELEV,DPRES,0.D0,DTEMP,  
     1           DOLAT,DELEVO,
     2           DDRY)  
      CALL METO3(DELEV,DPRES,DRH,DTEMP, 
     1             DOLAT,DELEVO,  
     2             DATMM)   
D     WRITE(1,9902) DDRY,DATMM
D9902 FORMAT("DDRY,DATMM="2D10.6) 
      WETD  = (DATMM - DDRY) *100.0   
      RETURN
      END 
