FTN4,X
C@SETSP 
C 
      SUBROUTINE SETSP(IDIR,ISP,IP),SET TAPE SPEED  C#870115:04:34# 
C 
      DIMENSION IBUF(10),IP(1)
C 
      IBUF(1) = 0 
      IBUF(2) = 2HTP
      CALL MV2MA(IBUF(3),IDIR,ISP,3H720)
      ICLASS = 0
      CALL EXEC(20,0,IBUF,-13,2HFS,0,ICLASS)
      CALL EXEC(23,6HMATCN ,ICLASS,1) 
      CALL RMPAR(IP)
      ICLASS = IP(1)
      CALL CLRCL(ICLASS)
      IP(1) = 0 
      IP(2) = 0 
      RETURN
      END 
