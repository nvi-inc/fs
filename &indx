FTN4
      INTEGER FUNCTION INDX(LIST,MLIST,NLIST,LS,IL) 
C 
C  LIST LOOK UP FOR TR-85 ANTCN 
C 
C  WEH 840620 
C 
C  INPUT: 
C 
      DIMENSION LIST(5,MLIST) 
C 
C         LIST - LIST TO  BE SEARCHED 
C 
C         MLIST - MAXIMUM NUMBER OF LIST ENTRIES
C 
C         NLIST - ACTUAL NUMBER OF ENTRIES
C 
C         LS - STRING TO BE SEARCHED  FOR 
C 
C         IL - THE NUMBER OF CHARCTERS TO COMPARE AT EACH POSTION 
C 
C  OUTPUT:
C 
C         INDX - POSITION OF LS IN LIST 
C 
C              = 0 IF NOT FOUND 
C 
      DIMENSION LS(5) 
C 
C  INITIALIZE 
C 
D     WRITE(1,992) MLIST,NLIST,(LS(I),I=1,5),IL 
D992  FORMAT(" INDX: MLIST, NLIST, LS, IL ",2I5,2X,5A2,I7)
D     IF(NLIST.GT.0) WRITE(1,991) ((LIST(I,J),I=1,5),J=1,NLIST) 
D991  FORMAT(" INDX : LIST ENTRY ",5A2) 
      INDX=0
C 
      IF(NLIST.LE.0.OR.NLIST.GT.MLIST) GO TO 10000
C 
      DO 10 I=1,NLIST 
          INDX=I
          IF(ICHCM(LIST(1,I),1,LS,1,IL).EQ.0) GO TO 10000 
10        CONTINUE
C 
C  NOT FOUND
C 
      INDX=0
      GO TO 10000 
C 
C RETURN
C 
10000 CONTINUE
D     WRITE(1,993) INDX 
D993  FORMAT(" INDX: INDX ",I9) 
      RETURN
      END 
