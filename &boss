FTN4,X
      PROGRAM BOSS(3,90),FIELD SYSTEM BOSS PROGRAM C#870115:04:16#
C 
C     DATE   WHO CHANGES
C     810906 NRV Add procedure name arrays
C 
      INCLUDE #FSCOM::FS
C 
      DIMENSION IP(5) 
C                   For RMPAR parameters
      DIMENSION LNAMES(5,85),LPROC1(5,80),LPROC2(5,80)
C                   Names list and empty procedure list 
      DIMENSION ITSCB(13,15)
C                   Time-scheduling control block 
      DIMENSION IDCBSK(144) 
C                   DCB for schedule
      DATA NTSCB/15/
C                     Number of available entries in ITSCB
      DATA MAXPR1/80/, MAXPR2/80/ 
C                     Number of entries in each proc list 
      DATA NNAMES/85/ 
C                     Maximum number of entries available in LNAMES 
C 
      CALL RMPAR(IP)
C 
      CALL RPLNK(1,6HBSEG1 ,1,7,IP,LNAMES,NNAMES,ITSCB,NTSCB, 
     .     IDCBSK,IERR) 
      IF (IERR.NE.0) GOTO 900 
100   CALL RPLNK(2,6HBSEG2 ,1,10,IP,LNAMES,NNAMES,LPROC1,MAXPR1,
     .LPROC2,MAXPR2,ITSCB,NTSCB,IDCBSK) 
900   CONTINUE
      CALL RPLNK(0,0.D0)
      CALL EXEC(6,0,3)
C                   HARI-KARI 
      END 
