FTN77,I,Y  
$CDS ON
      LOGICAL FUNCTION KCRT(LUT,IERR,IPBUF,JERR)
      LOGICAL KFMP
      CHARACTER*(*) IPBUF
C
      INTEGER   LCRET (  6 )
C
      DATA LCRET  /   9,2Hcr,2Hea,2Hti,2Hng,2H_ /
      KCRT=.FALSE.
      IF(JERR.EQ.IERR) RETURN
      KCRT=KFMP(LUT,IERR,LCRET(2),LCRET(1),IPBUF,1,0)
      RETURN
      END
