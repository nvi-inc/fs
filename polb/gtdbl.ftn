FTN77,I,Y  
$CDS ON
      DOUBLE PRECISION FUNCTION GTDBL(JBUF,IFC,ILC,IFIELD,IFERR)
      DOUBLE PRECISION DAS2B
      IFIELD=IFIELD+1
      CALL GTFLD(JBUF,IFC,ILC,IC1,IC2)
      GTDBL=DAS2B(JBUF,IC1,IC2-IC1+1,JERR)
      IF((IC1.LE.0.OR.JERR.NE.0).AND.IFERR.GE.0) IFERR=-IFIELD
      RETURN
      END
