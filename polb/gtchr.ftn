FTN77,I,Y  
$CDS ON
      SUBROUTINE GTCHR(LOUT,NF,NC,JBUF,IFC,ILC,IFIELD,IFERR)
      IFIELD=IFIELD+1
      CALL GTFLD(JBUF,IFC,ILC,IC1,IC2)
      IF(IC1.LE.0.AND.IFERR.GE.0) IFERR=-IFIELD
      CALL IFILL(LOUT,NF,NC,2H  )
      CALL ICHMV(LOUT,NF,JBUF,IC1,MIN0(NC,IC2-IC1+1))
      RETURN
      END
