FTN77,I,Y
$CDS ON
        SUBROUTINE IGPRM(IP,IIP,IIPN,LU)
     .,C#870328:18:46    MERGE PARMS V=2.1# 
        LOGICAL KBIT 
        DIMENSION IP(27),IIP(8)
        IPRES=JCHAR(IIPN,1) 
        IF(IPRES .EQ. 0)IPRES=174B
        IEND=MIN0(5,JCHAR(IIPN,2))
        DO 1000 I=1,IEND
        IF(KBIT(IPRES,8-I))IP(I)=IIP(I)
        IF(LU .NE. 0 .AND. IP(I) .EQ. 2Hlu)IP(I)=LU 
1000    CONTINUE
        RETURN
        END 
        END$
