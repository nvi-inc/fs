FTN4,X
        SUBROUTINE ISCHD(NAME,IDT,IRES),C#870115:05:49    t SCHED PRM#
C 
C               THIS ROUTINE TIME SCHEDULES PROGRAM 'NAME' TO 
C       RUN AT TIME RESOLUTION  IRES WITH INTERVAL IDT - REPEADEDLY 
C 
        DIMENSION IT(6) 
C 
C               GET CURRENT TIME
C 
        CALL EXEC(11,IT,IT(6))
C 
C               STEP TO NEXT TIME TO RUN
C 
        IF(IRES .NE. 4 .AND. IDT .LT. 24)CALL IADT(IT,IDT,IRES) 
C 
C               FOR EFFECTIVE TIME SCHED REMOVAL AN OLT TIME IS USED
C 
C               ADD THE PROGRAM TO TIME LIST
C 
        CALL EXEC(12,NAME,IRES,IDT,IT(4),IT(3),IT(2),0) 
C 
C               RETURN TO CALLER
C 
        RETURN
C 
        END 
        END$
