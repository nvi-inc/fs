FTN4
C@QRSOR 
      PROGRAM QRSOR,5 
C     This segment of QUIKR deals with source positions and offsets.
      CALL LINQB(NSUB)
      GOTO (1,2,3,4,5,6) NSUB 
1     CALL LINQC
      CALL SORCE(IP)
      GOTO 900
2     CALL LINQC
      CALL RDOFF(IP)
      GOTO 900
3     CALL LINQC
      CALL AEOFF(IP)
      GOTO 900
4     CALL LINQC
      CALL ONSOR(IP)
      GOTO 900
5     CALL LINQC
      CALL AZEL(IP) 
      GOTO 900
6     CALL LINQC
      CALL XYOFF(IP)
900   CALL LINQD
      END 
