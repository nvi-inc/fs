FTN77,I,Y  
$CDS ON
      LOGICAL FUNCTION KPOUT(LUT,IDCB,IBUF,NWDS,IOBUF,LST)
      LOGICAL KWRIT
      CHARACTER*(*) IOBUF
C
      IWDS=NWDS*2
      IF(NWDS.LT.0) IWDS=-NWDS
      IF(LST.GT.0) CALL EXEC(2,LST,IBUF,-MIN0(IWDS,78))
C
      CALL FmpWrite(IDCB,IERR,IBUF,IWDS)
      KPOUT=KWRIT(LUT,IERR,IOBUF)
      RETURN
      END
