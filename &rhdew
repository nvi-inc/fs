FTN4,X
      FUNCTION RHDEW(TEMP,DEWPT),C#820609:18:17             # 
C 
C     WILL COMPUTE RELATIVE HUMIDITY FROM DEW POINT AND DRY BULB
C     TEMPERATURES (I HOPE).  FROM SMITHSONIAN MET TABLES (1963)
C 
C     BRS - 10 DEC 79 
C 
      E(T) = 10.0**((-7.90298*(373.16/(T+273.16-1))) +
     1              ( 5.02808*ALOGT(373.16/(T+273.16))))
      RHDEW = (E(DEWPT)/E(TEMP))*100
      RETURN
      END 
